{%- endstyle -%}

{%- if template contains 'collection' -%}
  {%- render 'critical-style-collection' -%}
{%- elsif template contains 'product' -%}
  {%- render 'critical-style-product' -%}
{%- else -%}
  {%- render 'critical-style-general' -%}
{%- endif -%}

