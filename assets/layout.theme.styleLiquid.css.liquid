{%- if settings.enable_image_hover -%}

     .grid__item-image-wrapper {
		background-position: center;
		background-repeat: no-repeat;
		background-size: contain;
    } 
	 .grid__item-image-wrapper img {
		transition: all .1s linear;
	}
	 .grid__item-image-wrapper img:nth-of-type(1),
	 .grid__item-image-wrapper:hover img:nth-of-type(2) {
		opacity: 0;
		visibility: hidden;
	}
	 .grid__item-image-wrapper:hover img:nth-of-type(1) {
		opacity: 1;
		visibility: visible;
	}
	body .grid__item-image-wrapper .grid__item-image.one{
		opacity: 1;
		visibility: visible;
	}
	body .grid__item-image-wrapper .grid__item-image{
		opacity: 1;
		visibility: visible;
	}
	
	@media (max-width: 767px) {
		 .grid-uniform{
			justify-content: center;
		}
	}
